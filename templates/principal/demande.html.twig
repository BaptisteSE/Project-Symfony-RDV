{% extends 'base.html.twig' %}

{% block title %}Page de demande{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
    .card{
        margin-left: auto;
        margin-right: auto;
        background: rgb(180, 180, 180,0.4);
        max-width: 50em;
    }
    .title-demande{
        text-align: center
    }
    .btn{
        text-align: center
    }
</style>
<h1 class="title-demande">Demande de rendez-vous</h1>
<div class="card" style="width: 25em;">
    {{ form_start(form) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}
</div>




{% endblock %}
